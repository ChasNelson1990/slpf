{
  "name": "@gliff-ai/slpf",
  "version": "1.1.0",
  "description": "Scan Line Polygon Filling for gliff.ai apps",
  "main": "dist/main.js",
  "module": "dist/src/index.ts",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "webpack --progress --mode=development --config webpack.config.js",
    "format": "npx --no-install prettier --write src/*.ts",
    "prettier": "npx --no-install prettier -c src/*.ts",
    "lint": "npm run lint:ts && npm run prettier",
    "lint:ts": "npx eslint src/*.ts",
    "test": "jest --coverage --passWithNoTests",
    "prepublishOnly": "rm -r dist/; webpack --progress --mode=production --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gliff-ai/slpf.git"
  },
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/gliff-ai/slpf/issues"
  },
  "homepage": "https://github.com/gliff-ai/slpf#readme",
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-webpack-plugin": "^2.5.2",
    "jest": "^27.0.3",
    "prettier": "=2.3.0",
    "ts-loader": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.27.2",
    "webpack-cli": "^4.5.0"
  },
  "peerDependencies": {},
  "dependencies": {}
}
